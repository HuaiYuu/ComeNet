<script>
	
	fetch('URL_TO_YOUR_API_ENDPOINT')
		.then(response => response.json())
		.then(data => {
			const teamMembersContainer = document.getElementById('team-members');
			data.forEach(member => {
				const memberHtml = `
						<div class="col-lg-2 col-sm-4 col-xs-12 wow fadeInUp" data-wow-duration="1s" data-wow-delay="0.1s" data-wow-offset="0">
							<div class="our-team">
								<div class="single-team">
									<img src="${member.avatar}" class="img-fluid" alt="${member.name}">
									<h3>${member.name}</h3>
									<p>${member.role}</p>
								</div>
								<ul class="social">
									<li><a href="${member.facebook}" class="fa fa-facebook facebook"></a></li>
									<li><a href="${member.twitter}" class="fa fa-twitter twitter"></a></li>
									<li><a href="${member.google}" class="fa fa-google google"></a></li>
								</ul>
							</div>
						</div>`;

				teamMembersContainer.innerHTML += memberHtml;
			});
		})
		.catch(error => {
			console.error('Error fetching team member data:', error);
		});
</script>